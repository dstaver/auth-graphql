{
  "name": "pgtest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p start:*",
    "start:react": "react-scripts start",
    "start:pg": "nodemon ./server.js",
    "start:codegen": "gql-gen --config codegen.yml --watch",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "run-p lint:*",
    "lint:js": "eslint --ext js,ts,tsx src",
    "lint:ts": "tsc --noEmit",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "lint-fix": "run-p lint-fix:*",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint-fix:ts": "npm run lint:ts -- --fix",
    "types": "apollo client:codegen generate --config apollo.config.js --includes ./src/**/queries.ts --addTypename --target typescript --outputFlat src",
    "eject": "react-scripts eject",
    "graphql-generate": "gql-gen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@types/jest": "24.0.17",
    "@types/node": "12.6.9",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/yup": "^0.26.22",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.17.1",
    "formik": "^1.5.8",
    "graphql": "^14.4.2",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-typescript-client": "^0.18.2",
    "graphql-codegen-typescript-common": "0.18.2",
    "graphql-codegen-typescript-react-apollo": "^0.18.2",
    "graphql-codegen-typescript-server": "^0.18.2",
    "node-sass": "^7.0.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "postgraphile": "^4.4.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-apollo": "^3.0.0",
    "react-dom": "^16.8.6",
    "react-jsonschema-form": "^1.7.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "typescript": "3.5.3",
    "yup": "^0.27.0"
  }
}
